<p id="notice"><%= notice %></p>

<h1>Listing Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.email %></td>
        <td><%= feedback.content %></td>
        <td><%= link_to '查看', "feedbacks/#{feedback.id}/show" %></td>
        <td><%= link_to '删除', "feedbacks/#{feedback.id}", method: :delete, data: { confirm: '你确定要删除此条反馈吗?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

